{
  "name": "react-scroll-to",
  "version": "1.2.2",
  "description": "Scroll to a position using react",
  "main": "./dist/react-scroll-to.js",
  "types": "./dist/definitions",
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganderzz/react-scroll-to.git"
  },
  "keywords": [
    "react",
    "scroll",
    "to",
    "window",
    "position"
  ],
  "author": "Dylan Paulus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganderzz/react-scroll-to/issues"
  },
  "homepage": "https://github.com/ganderzz/react-scroll-to#readme",
  "peerDepedencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "prettier": "1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "jest": {
    "setupFiles": [
      "./src/tests/setup/shim.js",
      "./src/tests/setup/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!src/{setupTests,shim}.js",
      "!src/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "src/*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
